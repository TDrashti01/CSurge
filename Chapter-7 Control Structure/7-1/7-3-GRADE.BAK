#include <stdio.h>
#include <conio.h>

main()
{
	int marks;

	clrscr();

	printf("\n\n\tEnter your Marks :");
	scanf("%d", &marks);

	if (marks >= 91 && marks <= 100)
	{
		printf("\n\tGrade = A1");
	}
	else if (marks >= 81 && marks <= 90)
	{
		printf("\n\tGrade = A2");
	}
	else if (marks >= 71 && marks <= 80)
	{
		printf("\n\tGrade = B1");
	}
	else if (marks >= 61 && marks <= 70)
	{
		printf("\n\tGrade = B2");
	}
	else if (marks >= 51 && marks <= 60)
	{
		printf("\n\tGrade = C1");
	}
	else if (marks >= 41 && marks <= 50)
	{
		printf("\n\tGrade = C2");
	}
	else if (marks >= 33 && marks <= 40)
	{
		printf("\n\tGrade = D1");
	}
	else if (marks >= 0 && marks <= 32)
	{
		printf("\n\tYou Are Failed....");
	}
	else
	{
		printf("\n\tInvalid Marks !!!");
	}

	getch();
}